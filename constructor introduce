class Person{
    String name;
    int age;
public Person(String name,int age){
    this.name=name;
    this.age=age;
}
public void Introduce() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
        System.out.println("my name is name is Himaaya my age is 18");
}
public static void main(String[] args) {
    Person person = new Person("HIMAAYA", 18);
    person.Introduce();
    }
}
